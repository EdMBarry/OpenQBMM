FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [1 -1 -2 0 0 0 0];

internalField       uniform 1e5;

boundaryField
{
    inlet1
    {
        type            zeroGradient;
        value           $internalField;
    }
    inlet2
    {
        type            zeroGradient;
        value           $internalField;
    }
    outlet
    {
        type            fixedValue;
        p               $internalField;
        value           $internalField;
    }
    walls
    {
        type            fixedValue;
        value           $internalField;
    }
    defaultFaces
    {
        type    empty;
    }
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
